{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                },
                {
                    "key": "Permissions-Policy",
                    "value": "camera=(), microphone=(), geolocation=()"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/:path((?!www\\.).*)",
            "has": [
                {
                    "type": "host",
                    "value": "geekytechh.in"
                }
            ],
            "destination": "https://www.geekytechh.in/:path*",
            "permanent": true
        }
    ]
}